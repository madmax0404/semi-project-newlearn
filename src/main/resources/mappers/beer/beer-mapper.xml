<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="beer">
	<select id="checkToday" resultType="beerStore">
		select * from beer_store
		where last_date = trunc(sysdate)
	</select>
	
	<insert id="insertBeer">
		insert into beer_store
		values(seq_beer.nextval, #{imgUrl}, trunc(sysdate))
	</insert>
</mapper>




